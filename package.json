{
  "packageManager": "pnpm@10.6.1",
  "name": "robotvrm",
  "version": "2.0.0",
  "private": true,
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "audit-install": "pnpm audit --fix && pnpm install",
    "dev": "next dev --experimental-https --experimental-https-key ./certificates/localhost+2-key.pem --experimental-https-cert ./certificates/localhost+2.pem",
    "dev-store": "cross-env NEXT_PUBLIC_ENV=store pnpm dev",
    "build": "next build",
    "build-store": "cross-env NEXT_PUBLIC_ENV=store next build",
    "start": "pnpm license && next build && next start",
    "start-store": "cross-env NEXT_PUBLIC_ENV=store pnpm start",
    "sync": "npx ts-node generateiOSConfig.ts && npx env-cmd -f .env env-cmd -f .env.local cap sync",
    "sync-production": "cross-env NODE_ENV=production pnpm sync",
    "android": "pnpm sync && npx env-cmd -f .env env-cmd -f .env.local cap run android",
    "android-production": "pnpm sync-production && cross-env NODE_ENV=production npx env-cmd -f .env env-cmd -f .env.local cap run android",
    "ios": "pnpm sync && npx env-cmd -f .env env-cmd -f .env.local cap run ios",
    "ios-production": "pnpm sync-production && cross-env NODE_ENV=production npx env-cmd -f .env env-cmd -f .env.local cap run ios",
    "export": "next export",
    "export-store": "cross-env NEXT_PUBLIC_ENV=store next export",
    "lint": "next lint",
    "lint-store": "cross-env NEXT_PUBLIC_ENV=store next lint",
    "lint:fix": "eslint --fix .",
    "format": "prettier --write .",
    "electron": "wait-on http://localhost:3000 && electron ./electron.mjs",
    "desktop": "cross-env NEXT_PUBLIC_ROBOTVRM_BACKGROUND_IMAGE_PATH=\"\" run-p dev electron",
    "license": "pnpm license-checker-rseidelsohn --excludePrivatePackages --relativeLicensePath --relativeModulePath --json > public/libraries_licenses.json"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^1.2.2",
    "@ai-sdk/azure": "^1.3.4",
    "@ai-sdk/cohere": "^1.2.2",
    "@ai-sdk/google": "^1.2.3",
    "@ai-sdk/google-vertex": "^2.2.5",
    "@ai-sdk/mistral": "^1.2.1",
    "@ai-sdk/openai": "^1.3.4",
    "@anthropic-ai/sdk": "^0.39.0",
    "@aparajita/capacitor-secure-storage": "^6.0.1",
    "@capacitor-community/bluetooth-le": "^7.0.0",
    "@capacitor-community/camera-preview": "^7.0.0",
    "@capacitor/android": "^7.1.0",
    "@capacitor/core": "^7.1.0",
    "@capacitor/filesystem": "^7.0.0",
    "@capacitor/ios": "^7.1.0",
    "@charcoal-ui/icons": "^4.4.0",
    "@google-cloud/text-to-speech": "^6.0.1",
    "@google/generative-ai": "^0.24.0",
    "@headlessui/react": "^2.2.0",
    "@heroicons/react": "^2.2.0",
    "@marp-team/marp-core": "^4.0.1",
    "@marp-team/marpit": "^3.1.2",
    "@openrouter/ai-sdk-provider": "^0.4.5",
    "@pixiv/three-vrm": "^3.3.6",
    "@tailwindcss/line-clamp": "^0.4.4",
    "ai": "^4.2.8",
    "axios": "^1.8.4",
    "canvas": "^3.1.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.7",
    "env-cmd": "^10.1.0",
    "formidable": "^3.5.2",
    "groq-sdk": "^0.17.0",
    "i18next": "^24.2.3",
    "js-tiktoken": "^1.0.19",
    "license-checker-rseidelsohn": "^4.4.2",
    "next": "^15.2.4",
    "ollama-ai-provider": "^1.2.0",
    "openai": "^4.90.0",
    "pdfjs-dist": "^5.0.375",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-i18next": "^15.4.1",
    "three": "^0.175.0",
    "ts-node": "^10.9.2",
    "zod": "^3.24.2",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@capacitor/cli": "^7.1.0",
    "@charcoal-ui/tailwind-config": "^4.4.0",
    "@gltf-transform/core": "^4.1.3",
    "@types/dom-speech-recognition": "^0.0.5",
    "@types/formidable": "^3.4.5",
    "@types/node": "^22.13.14",
    "@types/react": "^19.0.12",
    "@types/react-dom": "^19.0.4",
    "@types/three": "0.175.0",
    "autoprefixer": "^10.4.21",
    "electron": "^35.1.2",
    "electron-is-dev": "^3.0.1",
    "eslint": "^9.23.0",
    "eslint-config-next": "^15.2.4",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-prettier": "^5.2.5",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.5.3",
    "prettier": "^3.5.3",
    "sass": "^1.86.0",
    "tailwindcss": "^3.4.17",
    "typescript": "5.8.2",
    "wait-on": "^8.0.3"
  },
  "engines": {
    "node": "22.8.0"
  }
}
